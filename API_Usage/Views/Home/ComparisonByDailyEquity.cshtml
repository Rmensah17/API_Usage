@model API_Usage.Models.EQComparison

@{
    ViewData["Title"] = "ComparisonByDailyEquity";
}

<h2>ComparisonByDailyEquity</h2>

<h4>EQComparison</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ComparisonByDailyEquity">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<table>
    <thead>
        <tr>
            <th>
                Symbol
            </th>
            <th>
                Name
            </th>
            <th>
                Date
            </th>
            <th>
                Type
            </th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model.FirstDailyEQ)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.symbol)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.marketaverage)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.markethigh)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.marketlow)
                </td>
            </tr>
        }
</table>

<table>
    <thead>
        <tr>
            <th>
                Symbol
            </th>
            <th>
                Name
            </th>
            <th>
                Date
            </th>
            <th>
                Type
            </th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model.SecondDailyEQ)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.symbol)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.marketaverage)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.markethigh)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.marketlow)
                </td>
            </tr>
        }
</table>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
